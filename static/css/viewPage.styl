/*表格以及表格的操作样式*/
color = red
ul,li
    list-style none
.formAction
    line-height 48px
    margin-bottom 10px
    overflow hidden
    .tableformitem
        width 33.3%
        float left
        .tableformitemlebel
            padding-right 10px
            float left
            text-align right
        .tableformitemInput
            float left
    .tableformitemTime
        width 66.6%
        float left
        .tableformitemlebel
            padding-right 10px
            float left
            text-align right
        .tableformitemInput
            float left
    .actionButtons
        margin-left 10px
        float right
@media (max-width: 1920px)
    .formAction
        .tableformitem 
            .tableformitemlebel
                width 30%
            .tableformitemInput
                width: 70%
        .tableformitemTime
            .tableformitemlebel
                width 15%
            .tableformitemInput
                width: 50%
@media (max-width: 1440px)
    .formAction
        .tableformitem 
            .tableformitemlebel
                width 40%
            .tableformitemInput
                width: 60%
        .tableformitemTime
            .tableformitemlebel
                width 20%
            .tableformitemInput
                width: 60%
@media (max-width: 1100px)
    .formAction
        .tableformitem 
            .tableformitemlebel
                width 55%
            .tableformitemInput
                width: 45%
        .tableformitemTime
            .tableformitemlebel
                width 27.5%
            .tableformitemInput
                width: 70%
.paging
    float right
    margin-bottom 30px
.dialogTitle
    font-size 16px
    font-weight 700
    padding-left 10px 
    border-bottom 1px solid black
    margin-bottom 5px
.line
    width 100%
    padding 4px 10px 0
    line-height 40px
    overflow hidden
    .doubleRow
        float left 
        width 50%
        overflow hidden
    .itemtitle
        float left
        width 120px
        padding-left 10px
    .itemframe
        float left
        width 320px
        .dataItem
            display inline-block
            width 225px
    .itemframebackup
        float left
        width 400px
    .notNecessary
        float left
        width 790px
    .is-disabled
        .el-input__inner,
        .el-textarea__inner
        .el-radio__inner
            background-color #fafafa
            color #999
.determine
    display none
.gou
    width 100%
    height 100%
    background url("../img/gou.png") 50% 50%/ auto no-repeat
.cuo
    width 100%
    height 100%
    background yellow
    background url("../img/cuo.png") 50% 50%/ auto no-repeat
.el-table
    margin 10px 0
.el-dialog__body
    padding 10px 20px
.el-dialog__headerbtn
    font-size 36px
.neccessary
    position relative
    top 0
    left 0
    &:before 
        content "*"
        display block
        position absolute
        color red
        left -10px
        top 0
        font-size 16px
